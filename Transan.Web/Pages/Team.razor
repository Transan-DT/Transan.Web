@page "/Team"
@layout CoverLayout
@inject HtmlContentFileLoader Loader

@HtmlContentFileLoader.LoadHtmlContent(Path.Combine("team", "content.html"))


@code {
	[CascadingParameter] CoverLayout Layout { get; set; }


	protected override void OnParametersSet()
	{
		Layout.SetCover("Notre Équipe", "Découvrez les personnes derrière le Transanctuaire.");
		base.OnParametersSet();
	}
}