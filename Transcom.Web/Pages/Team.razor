@page "/Team"
@layout CoverLayout

@Transcom.Web.Services.TeamPageLoaderService.HtmlContent


@code {
	[CascadingParameter] CoverLayout Layout { get; set; }

	const string PageTitle = "Notre Équipe";
	const string PageSubtitle = "Découvrez les personnes derrière Transgenres Community.";


	protected override void OnParametersSet()
	{
		if (Layout.CoverTitle is null or not PageTitle)
		{
			Layout.SetCover(PageTitle, PageSubtitle);
			Layout.Refresh();
		}
	}
}